- provide :header do
  = render 'showcase'
- provide :content_header, render('site/widgets/footer/overview')

.row
  .col-xs-6
    = render 'site/widgets/posts' if @lastest_posts.any?
    = render 'site/widgets/graduate' if @graduate_users.any?
    = render 'site/widgets/theses' if @lastest_theses.any?

  .col-xs-6
    = render 'site/widgets/events' if @upcoming_events.any?
    = render 'site/widgets/moments' if @recent_moments.any?
