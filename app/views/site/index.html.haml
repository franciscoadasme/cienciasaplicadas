- provide :header do
  = render 'showcase'

.row
  .col-xs-6
    = render 'site/widgets/overview' if @group.overview.present?
    = render 'site/widgets/events' if @upcoming_events.any?
    = render 'site/widgets/graduated' if @graduated_users.any?

  .col-xs-6
    = render 'site/widgets/posts'
    = render 'site/widgets/moments'
